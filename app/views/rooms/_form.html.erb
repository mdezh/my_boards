<turbo-frame id="<%= dom_id(@room) %>">
  <%= form_with(model: @room) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :description %>
    <%= f.text_area :description %>

    <%= f.submit "Submit" %>
    <%= link_to "Cancel", room_path(@room) %>
  <% end %>
</turbo-frame>
